{
  "name": "occam-parsers",
  "author": "James Smith",
  "version": "14.0.21",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/djalbat/occam-parsers",
  "description": "Occam's parsers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/occam-parsers"
  },
  "dependencies": {
    "necessary": "^7.1.9",
    "occam-lexers": "^14.0.23"
  },
  "devDependencies": {
    "@swc/core": "^1.2.50",
    "easy": "^11.1.22",
    "easy-layout": "^5.0.23",
    "easy-with-style": "^3.0.30",
    "esbuild": "^0.9.6",
    "express": "^4.17.1",
    "juxtapose": "^3.0.22",
    "lively-cli": "^2.0.7",
    "watchful-cli": "^1.6.15"
  },
  "scripts": {
    "start": "node ./bin/main.js",
    "clean": "rm -rf ./lib",
    "watchful": "watchful -mp=4 --transpiler=swc --bundler=esbuild --source-directory=./src --lib-directory=./lib --entry-file=example.js --bundle-file=./example.js --pause=100",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && npm run incremental",
    "watch-debug": "npm run clean && npm run batch-debug && npm run incremental-debug"
  },
  "main": "./lib/index.js"
}
